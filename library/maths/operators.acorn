import "maths/constants"

def sqrt(x as Float) as Float
  x
end

def sum{E, T}(sequence as E) as T
  let result as T = 0
    do
      for element in sequence
        result += element
      end
      result
    end
  end
end

def abs{T}(x as T) as T
  if x < 0
    -x
  else
    x
  end
end

def factorial(n as Integer) as Integer
  if n == 0
    1
  else
    n * factorial(n - 1)
  end
end
