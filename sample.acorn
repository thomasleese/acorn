type builtin Int64
type builtin Float64
type builtin Void

type Int as Int64
type Float as Float64

def exit(code as Int) as Void
  ccall exit(Int) as Void using code
end

let pi{T} as T = 3.141

def builtin +(a as Int64, b as Int64) as Int64
def builtin +(a as Float64, b as Float64) as Int64

let a = pi{Float} * 2
let b = pi{Int} * 2

exit(b)
