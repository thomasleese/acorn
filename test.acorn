def exit(code as Int64)
  ccall exit(Int64) as Void using code
end

def modify_integer(n as Int64)
  n * 2
end

def modify_boolean(n as Bool)
  not(n)
end

def apply(f as Function{Method{Int64, Int64}}, n as Int64)
  f(n)
end

def apply(f as Function{Method{Bool, Bool}}, n as Bool)
  f(n)
end

apply(modify_boolean, true)
exit(apply(modify_integer, 9))
